<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.playing.table.pokerdata">

    <application
        android:name=".TaskApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AddMemberListActivity"
            android:label="@string/title_activity_add_member_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".DataDetailActivity" />
        <activity android:name=".DataActivity" />
        <activity android:name=".ShowDownActivity" />
        <activity android:name=".CardActivity" />
        <activity android:name=".PlayingActivity" />
        <activity android:name=".MemberPlayingActivity" />
        <activity
            android:name=".MemberChangeActivity"
            android:label="@string/title_activity_member_change"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".HandActivity" />
        <activity android:name=".AddMemberActivity" />
        <activity
            android:name=".TurnActivity"
            android:label="@string/title_activity_turn"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".TitleActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>